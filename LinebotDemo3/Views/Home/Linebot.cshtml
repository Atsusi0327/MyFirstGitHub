@{
    ViewData["Title"] = "Line Bot Panel";
}
<h1>@ViewData["Title"]</h1>
<div class="content-wrapper">
    <div class="container-fluid">
        <form>
            <div class="row">
                <div class="col-6">
                    <label>Channel Access Token:</label>
                    <input class="form-control" id="fieldToken" value="" placeholder="ChannelAccessToken" />
                    <label>UserID:</label>
                    <input class="form-control" id="fieldUserId" value="" placeholder="UserID" />
                    <label>Message to Send:</label>
                    <input class="form-control" id="fieldMessage" value="" placeholder="Message to Send" />
                    <br />
                    <button class="btn btn-primary" id="Button1" type="button">Push</button>
                    <br />
                    <span id="result"></span>
                </div>
            </div>
        </form>
    </div>
</div>